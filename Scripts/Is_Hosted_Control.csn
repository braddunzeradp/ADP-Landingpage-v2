<![CDATA[var pattern = /adp/;
var locurl = document.location;	
var exists = pattern.test(locurl);
if (exists) {
	$('#temp-footer').addClass('hide-group');
} else {
  $('#temp-footer').removeClass('hide-group');
}]]>
